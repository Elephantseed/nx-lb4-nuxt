{
  "env": {
    "tsConfig": "tsconfig.json"
  },
  "nodeVersion": "system",
  "fixturesFolder": "./src/fixtures",
  "integrationFolder": "./src/integration",
  "testFiles": "**/*.{feature,features}",
  "modifyObstructiveCode": false,
  "pluginsFile": "./src/plugins/index.js",
