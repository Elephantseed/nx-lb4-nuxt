{
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "nx-lb4",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "front-end": {
      "tags": ["scope:client"]
    },
    "front-end-e2e": {
      "tags": ["scope:client"],
      "implicitDependencies": ["front-end"]
    },
    "back-end": {
      "tags": ["scope:server"],
      "implicitDependencies": ["front-end"]
    }
  }
}
