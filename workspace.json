{
  "version": 2,
  "cli": {
    "defaultCollection": "@nx-plus/nuxt"
  },
  "defaultProject": "nuxt3-app",
  "generators": {
    "@nx-plus/nuxt": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "back-end": {
      "projectType": "application",
      "root": "apps/back-end",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "rimraf *.tsbuildinfo",
              "lb-tsc --incremental"
            ],
            "cwd": "apps/back-end",
            "parallel": false
          }
        },
        "start": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "node index.js"
            ],
            "cwd": "apps/back-end/dist",
            "parallel": false
          }
        },
        "dev": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "ts-node index.ts"
            ],
            "cwd": "apps/back-end",
            "parallel": false
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/back-end"
          ],
          "options": {
            "jestConfig": "apps/back-end/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/back-end/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    },
    "nuxt3-app": {
      "projectType": "application",
      "root": "apps/nuxt3-app",
      "targets": {
        "storybook": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "start-storybook -p 6006"
            ],
            "cwd": "apps/nuxt3-app",
            "parallel": false
          }
        },
        "build-storybook": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "build-storybook"
            ],
            "cwd": "apps/nuxt3-app",
            "parallel": false
          }
        },
        "preview": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "npx nuxi preview"
            ],
            "cwd": "apps/nuxt3-app",
            "parallel": false
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "npx nuxi build"
            ],
            "cwd": "apps/nuxt3-app",
            "parallel": false
          }
        },
        "start": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "PORT=8080 node .output/server/index.mjs"
            ],
            "cwd": "apps/nuxt3-app",
            "parallel": false
          }
        },
        "dev": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "npx nuxi dev -o -p 8080"
            ],
            "cwd": "apps/nuxt3-app",
            "parallel": false
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/nuxt3-app/**/*.{ts,js,vue}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "jest"
            ],
            "cwd": "apps/nuxt3-app",
            "parallel": false
          }
        }
      }
    },
    "nuxt-client-e2e": {
      "root": "apps/nuxt-client-e2e",
      "sourceRoot": "apps/nuxt-client-e2e/src",
      "projectType": "application",
      "targets": {
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "npx playwright test src/tests/* --config=playwright.config.ts"
            ],
            "cwd": "apps/nuxt-client-e2e",
            "parallel": false
          }
        },
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nuxt-client-e2e/cypress.json",
            "tsConfig": "apps/nuxt-client-e2e/tsconfig.e2e.json",
            "devServerTarget": "nuxt-client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nuxt-client:serve:production"
            }
          }
        },
        "e2e-headless": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nuxt-client-e2e/cypress.json",
            "tsConfig": "apps/nuxt-client-e2e/tsconfig.e2e.json",
            "devServerTarget": "nuxt-client:serve",
            "headless": true
          },
          "configurations": {
            "production": {
              "devServerTarget": "nuxt-client:serve:production"
            }
          }
        },
        "open-e2e-console": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "cypress open --config-file cypress.json"
            ],
            "cwd": "apps/nuxt-client-e2e",
            "parallel": false
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/nuxt-client-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  }
}
